{
  "name": "health-bot-dashboard",
  "version": "1.0.0",
  "description": "Microsoft Health Bot Dashboard - build# #{Build.BuildNumber}#",
  "repository": {
    "type": "git",
    "url": "https://mshealthil.visualstudio.com/_git/HealthBOT"
  },
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "test_and_debug": "mocha %NODE_DEBUG_OPTION% --recursive --require ts-node/register --bail ./test/*spec.js",
    "test": "cross-env NODE_ENV=unit_test NODE_CONFIG_DIR=../config mocha",
    "test:api": "mocha -r ts-node/register --exit ./api-test/**/*.spec.ts",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "postcover": "start chrome %cd%/coverage/lcov-report/index.html",
    "lint": "eslint --ignore-path .eslintignore -c .eslintrc --ext .ts .",
    "cred": "cd ../tools/credscan && CredentialScanner.exe -I ../../HealthBotDashboard -S ./Searchers/buildsearchers.xml -O ../results/BotCredScanRsult -Sp ./CredScanSuppressions.json",
    "validate": "npm run lint && npm run cred"
  },
  "author": "Amir Taub (Microsoft)",
  "license": "ISC",
