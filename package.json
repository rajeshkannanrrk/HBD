    "handy-redis": "^1.5.2",
    "healthbotcommon": "file:./HealthBotCommon",
    "helmet": "^3.6.0",
    "jsonschema": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^12.3.2",
    "markdown-it-link-attributes": "^3.0.0",
    "merge": "^2.1.0",
    "moment": "^2.18.1",
    "ms-rest": "^2.5.0",
